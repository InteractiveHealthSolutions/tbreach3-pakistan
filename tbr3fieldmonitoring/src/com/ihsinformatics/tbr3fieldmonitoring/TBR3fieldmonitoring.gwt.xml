<module rename-to='tbr3fieldmonitoring'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->

	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->
	<inherits name="com.gwtext.GwtExt" />
	<inherits name="com.ihsinformatics.tbr3fieldmonitoring.TBR3fieldmonitoring" />

	<inherits name="com.summatech.gwt.GwtTimepicker" />

	<!-- 	Logging	 -->
	<inherits name="com.google.gwt.logging.Logging"/>       
	
<!-- 	<define-property name="gwt.logging.firebugHandler" values="ENABLED, DISABLED"/> -->
<!-- 	<define-property name="gwt.logging.popupHandler" values="ENABLED, DISABLED"/> -->
	
	<set-property name="gwt.logging.logLevel" value="INFO"/>  
	<set-property name="gwt.logging.enabled" value="TRUE"/>       
	<set-property name="gwt.logging.popupHandler" value="ENABLED" />

	<!-- Other module inherits -->

	<!-- Specify the app entry point class. -->
	<entry-point
		class='com.ihsinformatics.tbr3fieldmonitoring.client.Tbr3fieldmonitoring' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

	<!-- allow Super Dev Mode -->
	<!-- <add-linker name="xsiframe"/> -->
	<stylesheet src="js/ext/resources/css/ext-all.css" />

	<set-configuration-property name='xsiframe.failIfScriptTag'
		value='FALSE' />
	<stylesheet src="js/ext/resources/css/ext-all.css" />
	<script src="js/ext/adapter/ext/ext-base.js" />
	<script src="js/ext/ext-all.js" />
	<stylesheet src="js/ext/resources/css/ext-all.css" />
	<script src="js/ext/adapter/ext/ext-base.js" />
	<script src="js/ext/ext-all.js" />

	<!-- <script src="js/ext/adapter/ext/ext-base.js"/> -->
